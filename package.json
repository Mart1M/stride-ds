{
  "name": "stride-ds",
  "version": "0.2.0",
  "private": false,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "styles"
  ],
  "sideEffects": [
    "styles/*"
  ],
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build:lib": "tsup && tsc --project tsconfig.build.json --emitDeclarationOnly",
    "build:styles": "mkdir -p styles && cp src/app/brands.css styles/brands.css",
    "prepublishOnly": "npm run build:lib && npm run build:styles",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "npm run build:lib && npm run build:styles && changeset publish"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.539.0",
    "next": "15.4.6",
    "react": "19.1.0",
    "react-aria-components": "^1.11.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@eslint/eslintrc": "^3",
    "@storybook/addon-docs": "^9.1.2",
    "@storybook/addon-links": "^9.1.2",
    "@storybook/addon-onboarding": "^9.1.2",
    "@storybook/nextjs": "^9.1.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "eslint-plugin-storybook": "9.1.2",
    "storybook": "^9.1.2",
    "tailwindcss": "^4",
    "tsup": "^8.3.5",
    "typescript": "^5"
  },
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
